{
  "name": "fabiopellati/zf-db-util",
  "description": "utility for working wit ZendFramework3 Db ",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Fabio Pellati",
      "email": "fabio.pellati@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable":true,
  "require": {
    "zendframework/zend-servicemanager": "^3",
    "zendframework/zend-db": "^2"
  },
  "autoload": {
    "psr-4": {
      "Zf\\DbUtil\\": "src/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit"
  },
  "extra": {
    "zf": {
      "component": [
        "Zf\\DbUtil"
      ]
    }
  },
  "require-dev": {
  }
}
